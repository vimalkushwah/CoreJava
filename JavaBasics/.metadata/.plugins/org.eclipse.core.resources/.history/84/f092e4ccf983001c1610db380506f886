package Com.Rays.Java;

public class Sumofinteger {

	public static void main(String[] args) {
		int f=0;
		System.out.println();
		System.out.println("between 100&200 div by 7");
		for (int e=100;e<=200;e++) {
		if  ( e %7==0) {
			System.out.println(e);
			f+=e;
		}
		
		}System.out.println("sum of all no=="+f);
	}

}
