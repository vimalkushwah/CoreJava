package Com.Rays.Java;

public class TestArray {

	public static void main(String[] args) {
		int[] a= {2,4,6,8,10};
		int[]ar= new int[5];
		for(int v:ar) {
			System.out.println(v);
		}
		int[] b= {1,2,3,4,5,6,7,8,9};
		int[]c=new int[4];
		System.arraycopy(b, 2, c, 0, 4);
		System.out.println(c);

	}

}
