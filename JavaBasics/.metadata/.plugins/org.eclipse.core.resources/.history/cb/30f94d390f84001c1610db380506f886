package Com.Rays.String;

public class TestEfElse {

	public static void main(String[] args) {
		int money=500;
		if (money<100) {
			System.out.println("I can't buy Pizza");
		}else if(money>=100) {
			System.out.println("I can buy medium Pizza");
		}else{
			System.out.println("I can buy Pizza");
			
		}

	}

}
